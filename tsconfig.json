{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"]
  },
  "include": ["src", "App.tsx"],
}
